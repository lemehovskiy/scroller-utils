{
  "name": "@lemehovskiy/scroller-utils",
  "version": "0.0.9",
  "description": "Scroller util functions",
  "homepage": "https://github.com/lemehovskiy/scroller-utils#readme",
  "keywords": [
    "utils",
    "scroller"
  ],
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc --watch",
    "build": "tsc",
    "test": "jest --silent",
    "test:watch": "npm run test -- --watch",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint src/ --ext .ts,.tsx --max-warnings=0",
    "lint:prettier": "prettier 'src/**/*.{ts,tsx}' --check",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint src/ --ext .ts,.tsx --fix --max-warnings=0",
    "fix:prettier": "prettier 'src/**/*.{ts,tsx}' --write",
    "prepare": "husky install",
    "typedoc": "typedoc --out docs src/index.ts"
  },
  "author": "lemehovskiy",
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "ts-loader": "^9.2.6",
    "typedoc": "^0.22.11",
    "typescript": "^4.4.4"
  }
}
